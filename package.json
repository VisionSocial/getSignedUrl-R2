{
    "name": "r2-signed-url",
    "version": "1.0.0",
    "description": "get signed url to upload temp files on aws s3",
    "private": true,
    "license": "private",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --files --respawn --pretty --debug --transpile-only src/index.ts",
        "watch": "tsc -w",
        "compile": "tsc",
        "start": "node dist/index.js",
        "dev2": "nodemon dist/index.js",
        "build": "tsc --build",
        "clean": "tsc --build --clean",
        "test": "jest",
        "docs": "npx typedoc"
    },
    "author": "TheMrP forked from Erik Viveros forked from Cairo G. Resendiz",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.315.0",
        "@aws-sdk/s3-request-presigner": "^3.315.0",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.1",
        "express-joi-validation": "^5.0.1",
        "helmet": "^5.0.2",
        "joi": "^17.6.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/bluebird": "^3.5.36",
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^27.4.1",
        "@types/joi": "^17.2.3",
        "@types/node": "^17.0.45",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^8.3.4",
        "axios": "^0.26.1",
        "jest": "^27.5.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "ts-node-dev": "^1.1.8",
        "tsc": "^2.0.4",
        "typescript": "^4.6.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "roots": [
            "test"
        ],
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testMatch": [
            "**/*.(ts|tsx)"
        ],
        "globals": {
            "ts-jest": {
                "babelConfig": true,
                "tsConfig": "jest.tsconfig.json"
            }
        },
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "text-summary"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/mocks.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/mocks.js"
        },
        "testEnvironment": "node"
    }
}
